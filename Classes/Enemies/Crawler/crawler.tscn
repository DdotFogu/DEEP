[connection signal="Entered" from="state_machine/move" to="state_machine/move" method="random_crawl_direction"]
[connection signal="entered_ceil" from="state_machine/move" to="state_machine/move/AnimationPlayer" method="play" binds= ["ceil"]]
[connection signal="exited_ceil" from="state_machine/move" to="state_machine/move/AnimationPlayer" method="play" binds= ["exit_ceil"]]
[connection signal="Entered" from="state_machine/fall" to="audio_component" method="play_audio" binds= ["crawler_scream"]]
[connection signal="landed_on_floor" from="state_machine/fall" to="state_machine/fall/AnimationPlayer" method="play" binds= ["landed"]]
[connection signal="landed_on_floor" from="state_machine/fall" to="audio_component" method="play_audio" binds= ["crawler_land"]]
[connection signal="wait_timer_timeout" from="state_machine/fall" to="state_machine/fall/AnimationPlayer" method="play" binds= ["timeout"]]
[connection signal="Entered" from="state_machine/dead" to="state_machine/dead/AnimationPlayer" method="play" binds= ["dead"]]
